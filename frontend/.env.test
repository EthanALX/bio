# Testing environment variables for Visual Bio Frontend

# API Configuration
NEXT_PUBLIC_API_URL=http://localhost:8000

# Testing Configuration
NODE_ENV=test

# Disable analytics and external services in tests
NEXT_PUBLIC_ANALYTICS_ID=
NEXT_PUBLIC_SENTRY_DSN=

# Feature flags for testing
NEXT_PUBLIC_ENABLE_MOCKING=true
NEXT_PUBLIC_ENABLE_ERROR_BOUNDARIES=true

# Database and external services (mocked in tests)
NEXT_PUBLIC_DATABASE_URL=mock://database
NEXT_PUBLIC_WEBSOCKET_URL=mock://websocket